package it.betacom.ComputerFactory;

public class Server extends Computer{
	
	public Server() {
		super();
		this.HD = 0;
		this.CPU = 0;
		this.RAM = 0;
	}
	
	@Override
	void getRAM() {
		this.RAM = 8;
		
	}

	@Override
	void getHD() {
		this.HD = 2000;
	}

	@Override
	void getCPU() {
		this.CPU = 5.0;
		
	}

}
