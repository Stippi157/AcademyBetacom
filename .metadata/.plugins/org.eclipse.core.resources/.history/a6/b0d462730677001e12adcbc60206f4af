		Calcolatrice c = new Calcolatrice();
		myLogger.info("L'operazione Ã¨ uguale a: " + c.somma(13,9));
		
		Calcolatrice c2 = new Calcolatrice();
		
		public String somma(int a, int b) {
			String messaggio = "La somma di " + a + "e " + b + " vale: ";
